{
    "name": "samuelvi/spreadsheet-translator-provider-googledriveauth",
    "license": "MIT",
    "type": "library",
    "description": "Spreadsheet Translator - Google Drive With Authentication Provider",
    "autoload": {
        "psr-4": {
            "Atico\\SpreadsheetTranslator\\Provider\\GoogleDriveAuth\\": "./src"
        }
    },
    "repositories": [{
        "type": "path",
        "url": "../../Core",
        "options": {
            "symlink": true
        }
    }],
    "require": {
        "samuelvi/spreadsheet-translator-core": "^1.0|^2.0|^8.0",
        "google/apiclient": "^2.0",
        "phpoffice/phpspreadsheet": "^1.5|^2.0",
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.7",
        "guzzlehttp/promises": "^2.0"
    },
    "require-dev": {
        "rector/jack": "^0.2.9",
        "rector/rector": "^2.1"
    }
}
